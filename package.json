{
	"name": "sveltekit-lucia-daisyui",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "prisma generate && vite dev",
		"build": "prisma generate && prisma db push && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"clean": "rm -rf node_modules .svelte-kit db-data",
		"init": "pnpm i && prisma db push",
		"reinit": "pnpm run clean && pnpm i && prisma db push",
		"refresh": "pnpm run clean && pnpm i"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.26.0",
		"postcss": "^8.4.23",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^4.13.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^5.0.3",
		"sveltekit-superforms": "^0.8.5",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0",
		"zod": "^3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^2.0.0",
		"@prisma/client": "^4.13.0",
		"daisyui": "^2.51.6",
		"lucia-auth": "^1.5.0"
	}
}
